<?xml version="1.0" encoding="UTF-8"?>
<udm.DeploymentPackage version="${RELEASE_VERSION}" application="pdok-featured">
  <orchestrator />
  <satisfiesApprovalByChangemanager>true</satisfiesApprovalByChangemanager>
  <satisfiesChangeTicketNumber>${CHANGE_NUMBER}</satisfiesChangeTicketNumber>
  <deployables>
    <tomcat.LoggingSpec name="featuredLogging">
      <tags />
      <logging>
        <entry key="IGNORE-ERROR">{{featured.logging.ignore-ERROR}}</entry>
        <entry key="DEFAULT-TRACE">{{featured.logging.default-TRACE}}</entry>
        <entry key="IGNORE-TRACE">{{featured.logging.ignore-TRACE}}</entry>
        <entry key="DEFAULT-ERROR">{{featured.logging.default-ERROR}}</entry>
        <entry key="DEFAULT-DEBUG">{{featured.logging.default-DEBUG}}</entry>
        <entry key="IGNORE-INFO">{{featured.logging.ignore-INFO}}</entry>
        <entry key="IGNORE-WARN">{{featured.logging.ignore-WARN}}</entry>
        <entry key="DEFAULT-INFO">{{featured.logging.default-INFO}}</entry>
        <entry key="DEFAULT-WARN">{{featured.logging.default-WARN}}</entry>
        <entry key="IGNORE-DEBUG">{{featured.logging.ignore-DEBUG}}</entry>
        <entry key="IGNORE-FATAL">{{featured.logging.ignore-FATAL}}</entry>
        <entry key="DEFAULT-FATAL">{{featured.logging.default-FATAL}}</entry>
      </logging>
    </tomcat.LoggingSpec>
    <tomcat.War name="featured-war" file="uberjar/featured.war">
      <tags />
      <scanPlaceholders>true</scanPlaceholders>
      <excludeFileNamesRegex>.*\.(jar|xsl|template)</excludeFileNamesRegex>
      <checksum>${RELEASE_VERSION}</checksum>
      <contextRoot>featured</contextRoot>
    </tomcat.War>
  </deployables>
</udm.DeploymentPackage>