{{>city-model-start}}
	<veg:SolitaryVegetationObject xmlns:veg="http://www.opengis.net/citygml/vegetation/2.0" gml:id="_{{_version}}">
		{{>start-bgt-feature-type}}
			<veg:class codeSpace="{{type_codeSpace}}">{{type}}</veg:class>		
			<imgeo:geometrie2dVegetatieObject>{{{geometry.gml}}}</imgeo:geometrie2dVegetatieObject>		
			<imgeo:plus-type codeSpace="{{plus-type_codeSpace}}">{{plus-type}}{{plus-type_leeg}}</imgeo:plus-type>		
	</veg:SolitaryVegetationObject>
{{>city-model-eind}}